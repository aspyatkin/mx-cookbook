<% @entries.each do |entry| %>
<%= entry[:name] %> {
  map = "<%= entry[:map] %>";
  <% entry.reject { |key| key == :name || key == :map }.each do |prop_name, prop_val| %>
  <%= prop_name %> = <% if prop_val.is_a?(String) %>"<% end %><%= prop_val %><% if prop_val.is_a?(String) %>"<% end %>;
  <% end %>
}
<% end %>
